  "scripts": {
    "next:dev": "yarn build:tailwind-dev && next dev",
    "next:build": "yarn build:tailwind && next build",
    "next:start": "next start",
    "lint": "eslint --ext js,jsx .",
    "vercel-build": "yarn build:tailwind && next build",
    "build:tailwind": "cross-env NODE_ENV=production tailwind build ./tailwind.css -o src/assets/tailwind.css",
